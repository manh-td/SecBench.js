test("Path Traversal in fbr-client", () => {
    const { exploitFunction } = require("./function.js");
    const { getPayload } = require('./payload.js');
    const fs = require("fs");

    const payload = getPayload();
    exploitFunction(payload);

    // Verify the output
    expect(fs.readFileSync(path.resolve(__dirname, "../flag.html")).toString()).toBe(
        fs.readFileSync(path.resolve(__dirname, "../flag.html")).toString()
    );
});